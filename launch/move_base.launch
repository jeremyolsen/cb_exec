<launch>

	<remap from="openni_points" to="/planner_cloud"/>
    <remap from="odom" to="/odometry/filtered/global" />
	<remap from="map" to="rtabmap/proj_map"/>
	<!-- <remap from="move_base_simple/goal" to="/planner_goal"/>   -->
	<!-- using the RTABMAP LIDAR map... -->

   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

 	      <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
 	      <param name="controller_frequency" value="8.0" />
 	      <param name="controller_patience" value="15.0" />
 	      <param name="planner_frequency" value="0.0" />
 	      <param name="clearing_rotation_allowed" value="false" />
 	      <param name="recovery_behavior_enabled" value="false" />

    	  <rosparam file="/home/nvidia/catkin_ws/src/cb_exec/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
     	  <rosparam file="/home/nvidia/catkin_ws/src/cb_exec/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    	  <rosparam file="/home/nvidia/catkin_ws/src/cb_exec/params/local_costmap_params.yaml" command="load" />
    	  <rosparam file="/home/nvidia/catkin_ws/src/cb_exec/params/global_costmap_params.yaml" command="load" />
    	  <rosparam file="/home/nvidia/catkin_ws/src/cb_exec/params/base_local_planner.yaml" command="load" />

   	</node>
</launch>
